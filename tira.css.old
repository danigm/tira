/* default skin */

body {
	text-align: center;
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	background-color: rgb(102, 153, 204);
}
body,td,div,img {
	font-family: Tahoma;
	font-size: 12px;
	color: #000000;
	vertical-align: top;
	text-align: center;
}
div {
	overflow: hidden;
	word-wrap: break-word; /* IE ONLY */
}
a:link {
	color: #0000ff;
}
a:visited {
	color: #333344;
}
a:hover {
	color: #ffffff;
}
a:active {
	color: #000000;
}
input {
	border: 1px solid #222222;
	background-color: #FAFAFA;
	font-family: Tahoma;
	font-size: 12px;
	color: #000000;
	text-align: center;
	margin-bottom: 1px;
	cursor: default;
}
button {
	border: 2px solid #333333;
	background-color: #eeeeee;
	font-family: Tahoma;
	font-size: 12px;
	color: #000000;
	text-align: center;
	margin-bottom: 1px;
	cursor: default;
}
textarea {
	border: 1px solid #222222;
	background-color: #FAFAFA;
	font-family: Tahoma;
	font-size: 12px;
	color: #000000;
	text-align: left;
	margin-bottom: 1px;
	cursor: default;
}
table,th,tr,td {
	border: 1px solid #222222;
	background-color: rgb(102, 153, 204);
	font-family: Tahoma;
	font-size: 12px;
	color: #000000;
	text-align: left;
	margin-bottom: 1px;
	cursor: default;
}
